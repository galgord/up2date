    "babel-plugin-module-resolver": "^5.0.0",
    "expo": "~50.0.2",
    "expo-constants": "~15.4.5",
    "expo-font": "~11.10.2",
    "expo-linking": "~6.2.2",
    "expo-localization": "~14.8.3",
    "expo-router": "~3.4.4",
    "expo-secure-store": "~12.8.1",
    "expo-status-bar": "~1.11.1",
    "graphql": "^16.8.1",
    "i18next": "^23.8.2",
